<?xml version="1.0" encoding="ASCII"?>
<platform:PlatformDefinition xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:platform="com.xatkit.platform" name="SlackPlatform" runtimePath="com.xatkit.plugins.slack.platform.SlackPlatform">
  <extends href="pathmap://XATKIT_CORE_PLATFORM/ChatPlatform.xmi#/"/>
  <eventProviderDefinitions xsi:type="platform:InputProviderDefinition" name="SlackIntentProvider">
    <outContexts name="slack" lifeSpan="1">
      <parameters name="channel"/>
      <parameters name="username"/>
      <parameters name="rawMessage"/>
      <parameters name="userId"/>
      <parameters name="userEmail"/>
      <parameters name="threadTs"/>
      <parameters name="messageTs"/>
    </outContexts>
  </eventProviderDefinitions>
  <eventProviderDefinitions xsi:type="platform:InputProviderDefinition" name="ChatProvider">
    <outContexts name="chat" lifeSpan="1">
      <parameters name="channel"/>
      <parameters name="username"/>
      <parameters name="rawMessage"/>
    </outContexts>
  </eventProviderDefinitions>
  <actions name="PostFileMessage">
    <parameters key="message"/>
    <parameters key="file"/>
    <parameters key="channel"/>
  </actions>
  <actions name="PostFileMessage">
    <parameters key="title"/>
    <parameters key="message"/>
    <parameters key="content"/>
    <parameters key="channel"/>
  </actions>
  <actions name="ReplyFileMessage">
    <parameters key="message"/>
    <parameters key="file"/>
  </actions>
  <actions name="PostAttachmentsMessage">
    <parameters key="attachments"/>
    <parameters key="channel"/>
  </actions>
  <actions name="ReplyAttachmentsMessage">
    <parameters key="attachments"/>
  </actions>
  <actions name="PostAttachmentsMessage">
    <parameters key="pretext"/>
    <parameters key="title"/>
    <parameters key="text"/>
    <parameters key="attchColor"/>
    <parameters key="timestamp"/>
    <parameters key="channel"/>
  </actions>
  <actions name="ReplyAttachmentsMessage">
    <parameters key="pretext"/>
    <parameters key="title"/>
    <parameters key="text"/>
    <parameters key="attchColor"/>
    <parameters key="timestamp"/>
  </actions>
  <actions name="PostAttachmentsMessage">
    <parameters key="pretext"/>
    <parameters key="title"/>
    <parameters key="text"/>
    <parameters key="attchColor"/>
    <parameters key="channel"/>
  </actions>
  <actions name="ReplyAttachmentsMessage">
    <parameters key="pretext"/>
    <parameters key="title"/>
    <parameters key="text"/>
    <parameters key="attchColor"/>
  </actions>
  <actions name="ItemizeList">
    <parameters key="list"/>
  </actions>
  <actions name="ItemizeList">
    <parameters key="list"/>
    <parameters key="formatter"/>
  </actions>
  <actions name="EnumerateList">
    <parameters key="list"/>
  </actions>
  <actions name="EnumerateList">
    <parameters key="list"/>
    <parameters key="formatter"/>
  </actions>
  <actions name="PostMessage">
    <parameters key="message"/>
    <parameters key="channel"/>
  </actions>
  <actions name="Reply">
    <parameters key="message"/>
  </actions>
  <imports path="ChatPlatform"/>
</platform:PlatformDefinition>
